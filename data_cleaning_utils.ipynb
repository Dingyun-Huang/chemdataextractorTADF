    "# Fix overlaping duplicates between AutoTableParser and TauDTableParser.\n",
    "\n",
    "# First group records by doi.\n",
    "\n",
    "def group_records_by_doi(d_list):\n",
    "    d_dict_by_doi = defaultdict(list)\n",
    "    for d in d_list:\n",
    "        current_doi = d['doi']\n",
    "        d_dict_by_doi[current_doi].append(d)\n",
    "    return d_dict_by_doi\n",
    "\n",
    "# Fix case of tau_p tau_d pairs and lambda_EM and tau_d pairs.\n",
    "\n",
    "def remove_atp_duplicates_tau(d_list):\n",
    "\n",
    "    d_dict_by_doi = group_records_by_doi(d_list)\n",
    "    d_dict_by_doi_1 = {}\n",
    "    for doi, ds in d_dict_by_doi.items():\n",
    "        processed_ds = []\n",
    "\n",
    "        if any([d[\"record_method\"] == \"TauDTableParser\" for d in ds]):\n",
    "            \n",
    "            for d1 in ds:\n",
    "                f = False\n",
    "\n",
    "                try:\n",
    "                    # case of tau_p / tau_d\n",
    "                    if \"/\" in d1['raw_value'] and \"AutoTableParser\" == d1['record_method']:\n",
    "                        value = d1['raw_value'].split(\"/\")[1]\n",
    "                        for d2 in ds:\n",
    "                            if \"TauDTableParser\" == d2['record_method'] and d2['raw_value'] in value:\n",
    "                                f = True\n",
    "                                break\n",
    "                    # case of lambda EM (tau_d)\n",
    "                    elif \"(\" in d1['raw_value'] and \"AutoTableParser\" == d1['record_method']:\n",
    "                        value = d1[\"raw_value\"].replace(\"(\", \" \").replace(\")\", \" \").split()\n",
    "                        if 300 < float(value[0]) < 1600:\n",
    "                            for d2 in ds:\n",
    "                                if \"TauDTableParser\" == d2['record_method'] and d2['raw_value'] in value:\n",
    "                                    f = True\n",
    "                                    break\n",
    "                except KeyError:\n",
    "                    pass\n",
    "\n",
    "                if not f:\n",
    "                    processed_ds.append(d1)\n",
    "        d_dict_by_doi_1[doi] = processed_ds\n",
    "    \n",
    "    result = []\n",
    "    for l in d_dict_by_doi_1.values():\n",
