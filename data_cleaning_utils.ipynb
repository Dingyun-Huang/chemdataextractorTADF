    "import numpy as np\n",
    "\n",
    "unit_map_tau = {'(10^-12.0) * Second^(1.0)': 1e-6,\n",
    " '(10^-3.0) * Second^(1.0)': 1e3,\n",
    " '(10^-6.0) * Second^(1.0)': 1.0,\n",
    " '(10^-9.0) * Second^(1.0)': 1e-3,\n",
    " 'Second^(1.0)': 1e6}\n",
    "\n",
    "# remove tau > 10 s and tau < 100 ns\n",
    "\n",
    "def remove_unphysical_tau(d_list):\n",
    "    \"\"\"\n",
    "    Remove delayed lifetime less than 100 ns or more than 10 s.\n",
    "    Input a list of dictionaries of raw STSplit data records from CDE\n",
    "    \"\"\"\n",
    "    d_list_large_cleaned = []\n",
    "    for i, d in enumerate(d_list):\n",
    "        try:\n",
    "            if \"Second\" in d[\"units\"] and \"value\" in d.keys():\n",
    "                if -1 < np.log10(st.mean(d[\"value\"]) * unit_map_tau[d[\"units\"]]) < 7:\n",
    "                    d_list_large_cleaned.append(d)\n",
    "            else:\n",
