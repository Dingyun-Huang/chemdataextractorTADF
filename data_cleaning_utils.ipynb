    "from scipy import constants\n",
    "\n",
    "def convert_molar_kJ(d_list):\n",
    "\n",
    "    d_list_joule_converted = copy.deepcopy(d_list)\n",
    "    for i, d in enumerate(d_list_joule_converted):\n",
    "        if \"kJ\" in d[\"raw_units\"]:\n",
    "            print(f\"{i}th record {d['value']}\")\n",
    "            d[\"value\"] = [round(v * 1000 / constants.Avogadro / constants.electron_volt, 3) for v in d[\"value\"]]\n",
    "            d[\"units\"] = \"ElectronVolt^(1.0)\"\n",
    "            d[\"units\"] = \"kJ/mol\"\n",
    "            print(d[\"value\"])"
    "\n",
    "    return d_list_joule_converted"
   ]
