   "source": [
    "# Mapping between the old and target JSON schema for emission wavelength data records\n",
    "schema_map_lambda = {'EmissionWavelength': {'raw_value': \"raw_value\", \n",
    "                      'raw_units': \"raw_units\",\n",
    "                      'value': \"value\",\n",
    "                      'units': 'units',\n",
    "                      'specifier': 'specifier',\n",
    "                      'compound': {'ThemeCompound': {'names': \"compound names\", \n",
    "                                                     'labels': \"compound labels\"}},\n",
    "                      \"phase\": {\"Phase\": {\"specifier\":\"phase specifier\",\n",
    "                                          \"host\":\"phase host\"}}},\n",
    "          'doi': \"doi\",\n",
    "          'record_method': 'record_method',\n",
    "          'c_merged': \"c_merged\"}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Unit conversion map to nanometers\n",
    "unit_map_lambda = {\n",
    " '(10^-6.0) * Meter^(1.0)': 1e3,\n",
    " '(10^-9.0) * Meter^(1.0)': 1.0,\n",
    " '(10^-3.0) * Meter^(1.0)': 1e6,\n",
    " '(10^-2.0) * Meter^(1.0)': 1e7,\n",
    " 'Meter^(1.0)': 1e9}\n",
    "\n",
    "def remove_unphysical_wavelength(d_list):\n",
    "    \"\"\"\n",
    "    Remove emission wavelength less than 300 nm or more than 1600 nm.\n",
    "    Input a list of dictionaries of raw EmissionWavelength data records from CDE\n",
    "    \"\"\"\n",
    "    d_list_cleaned = []\n",
    "    for i, d in enumerate(d_list):\n",
    "        try:\n",
    "            if \"value\" in d.keys():\n",
    "                if (d[\"raw_value\"].isnumeric() and \n",
    "                        (st.mean(d[\"value\"]) * unit_map_lambda[d[\"units\"]] > 1600 or \n",
    "                         st.mean(d[\"value\"]) * unit_map_lambda[d[\"units\"]] < 300)):\n",
    "                    pass\n",
    "                else:\n",
    "                    d_list_cleaned.append(d)\n",
    "            else:\n",
    "                d_list_cleaned.append(d)\n",
    "        except KeyError:\n",
    "            d_list_cleaned.append(d)\n",
    "    \n",
    "    return d_list_cleaned"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Some TADF molecules have multiple emission peaks, e.g., 524/616 nm.\n",
    "# The function prevents the phrase from being interpreted as a fraction.\n",
    "\n",
    "def infer_paired_values_lambda(d_list):\n",
    "    d_new = []\n",
    "    for i, d in enumerate(d_list):\n",
    "        if \"/\" in d[\"raw_value\"] and d[\"record_method\"] == \"AutoTableParser\":\n",
    "            values = d[\"raw_value\"].split(\"/\")\n",
    "            print(d[\"raw_value\"])\n",
    "            if float(values[1]) - float(values[0]) > 100:\n",
    "                d[\"value\"] = float(values[1])\n",
    "            else:\n",
    "                d_1 = copy.deepcopy(d)\n",
    "                d_1[\"value\"] = [float(values[1])]\n",
    "                d[\"value\"] = [float(values[0])]\n",
    "                d_new.append(d_1)\n",
    "                pass\n",
    "        \n",
    "        elif \"(\" in d[\"raw_value\"] and d[\"record_method\"] == \"AutoTableParser\":\n",
    "            values = d[\"raw_value\"].replace(\"(\", \" \").replace(\")\", \" \").split()\n",
    "            print(values)\n",
    "            if float(values[1]) < 300:\n",
    "                d[\"value\"] = [float(values[0])]\n",
    "            else:\n",
    "                if abs(float(values[1]) - float(values[0])) < 100:\n",
    "                    d_1 = copy.deepcopy(d)\n",
    "                    d_1[\"value\"] = [float(values[1])]\n",
    "                    d[\"value\"] = [float(values[0])]\n",
    "                    d_new.append(d_1)\n",
    "                else:\n",
    "                    d[\"value\"] = [float(max(values))]\n",
    "\n",
    "    d_list = d_list + d_new\n",
    "    return d_list"
   ]
  },
